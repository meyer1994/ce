

comprido fact(medio a) {
    se (a < 2) {
        devolve 1;
    }

    comprido mul = 1;
    para (medio i = 1; i <= a; i = i + 1) {
        mul = mul * i;
    }

    devolve mul;
}


medio sum(medio a, medio b) {
    devolve a + b;
}


medio main() {
    medio a = 10;
    medio b = 5;
    devolve fact(a - b);
}
